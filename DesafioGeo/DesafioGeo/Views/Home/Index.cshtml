@{
    ViewBag.Title = "Desafio GeoAmbiente";
}

<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&signed_in=true"></script>
<script src="~/Scripts/Base/dribbble.js"></script>

<script>
    $(document).ready(function () {
        ImagensPopularesPagina($("#pagina").val());
    });
</script>

<br />
<div id="container" class="container">
    <input type="hidden" id="pagina" name="pagina" value="0" />
    <div id="imgCentro">

    </div>
    <nav>
        <ul class="pager">
            <li><a href="javascript:;" onclick="ImagensPopularesPagina($('#pagina').val());">Mais Imagens</a></li>
        </ul>
    </nav>
</div>
<br />

<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel"></h4>
            </div>
            <div id="modalImg" class="modal-body">


            </div>
        </div>
    </div>
</div>
